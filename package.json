{
  "name": "wenke-dev",
  "version": "7.0.0",
  "description": "dev tool for wenke-webpack",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "test": "jest --coverage --silent",
    "prepublishOnly": "npm run test"
  },
  "bin": {
    "wenke-dev": "bin/wenke-dev"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mopduan/wenke-dev.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "babel",
    "es6",
    "es7",
    "es8",
    "webpack",
    "commonjs",
    "wenke",
    "wenke-dev"
  ],
  "author": "mopduan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mopduan/wenke-dev/issues"
  },
  "homepage": "https://github.com/mopduan/wenke-dev",
  "dependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "autoprefixer": "10.2.6",
    "babel-loader": "8.2.2",
    "chalk": "4.1.1",
    "chokidar": "3.5.2",
    "commander": "8.0.0",
    "css-loader": "5.2.6",
    "del": "6.0.0",
    "falafel": "2.2.4",
    "fonteditor-core": "2.1.7",
    "fork-ts-checker-webpack-plugin": "6.2.12",
    "glob": "7.1.7",
    "gm": "1.23.1",
    "imagemin": "7.0.1",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.2",
    "jest": "27.0.6",
    "minimatch": "3.0.4",
    "node-fetch": "2.6.1",
    "node-sass": "6.0.1",
    "postcss": "8.3.5",
    "postcss-assets": "6.0.0",
    "postcss-px-editor": "0.1.1",
    "postcss-pxtorem": "6.0.0",
    "postcss-url-editor": "0.3.3",
    "prettier": "2.3.2",
    "semver": "7.3.5",
    "shequfe-webfonts-generator": "0.5.0",
    "socket.io": "4.1.2",
    "spritesheet-templates": "10.5.2",
    "spritesmith": "3.4.0",
    "style-loader": "3.0.0",
    "ts-loader": "9.2.3",
    "typescript": "4.3.4",
    "uglify-js": "3.13.10",
    "underscore": "1.13.1",
    "url2": "1.0.4",
    "webpack": "5.41.1",
    "worker-farm": "1.7.0"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/__test__/setupTestFramework.js"
    ],
    "testMatch": [
      "<rootDir>/__test__/*.test.js",
      "<rootDir>/__test__/*.spec.js"
    ],
    "testEnvironment": "node"
  }
}